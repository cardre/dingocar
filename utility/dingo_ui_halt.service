[Unit]
Description=Turn power off after shutdown
DefaultDependencies=no
Before=umount.target

[Service]
Type=oneshot
ExecStart=/home/pi/play/ai/dingocar/utility/dingo_env.sh oled_message.py Halt
WorkingDirectory=/home/pi/play/ai/dingocar/utility
User=pi

[Install]
WantedBy=halt.target poweroff.target
